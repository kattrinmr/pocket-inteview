<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--EditText-->
    <style name="EditTextField" parent="@style/ThemeOverlay.Material3.TextInputEditText.OutlinedBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>

        <item name="boxStrokeWidth">0dp</item>

        <item name="endIconMode">clear_text</item>

        <item name="hintTextColor">@color/black</item>


        <item name="boxCornerRadiusTopStart">@dimen/round_normal</item>
        <item name="boxCornerRadiusTopEnd">@dimen/round_normal</item>
        <item name="boxCornerRadiusBottomStart">@dimen/round_normal</item>
        <item name="boxCornerRadiusBottomEnd">@dimen/round_normal</item>
    </style>

</resources>